.settings-box.hide-settings
  .toggle-settings
    i.fa.fa-gear.fa-lg
  .settings-content
    .option-box.color-options.text-center
      h4.option-title Color Options
      - var themes = ["GREEN-theme", "blue-theme", "BLUEBELL-theme","bluedefrance-theme", "RED-theme", "Pink-theme", "yellow-theme", "orange-theme" ]
      ul.list-unstyled
        each theme in themes
          li(data-theme=theme)
    .option-box
      h4.option-title Font Option
    .option-box
      h4.option-title Sidebar Option